<%= render 'shared/page_title', title: @category.name %>
<% if @category.movies %>
  <% @category.movies.each do |movie| %>
    <ul class="listing">
      <div class="row">
        <div class="well col-md-4 col-md-offset-4">
          <li class="chef-title"><%= movie.name.capitalized %></li>
          <p><%= movie.description %></p>
        </div>
      </div>
    </ul>
  <% end %>
<% end %>
